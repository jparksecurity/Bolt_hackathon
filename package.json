{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run typecheck && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --project tsconfig.app.json --noEmit",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:status": "npx supabase status",
    "supabase:reset": "npx supabase db reset",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "knip": "npx knip --production --fix --allow-remove-files",
    "supabase:gen:types": "npx supabase gen types typescript --linked --schema public > src/types/database.ts"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.31.9",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@supabase/supabase-js": "^2.50.0",
    "@types/luxon": "^3.6.2",
    "fractional-indexing": "^3.2.0",
    "lucide-react": "^0.344.0",
    "luxon": "^3.6.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.5.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "prettier": "^3.5.3",
    "supabase": "^2.24.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^6.3.5"
  }
}
